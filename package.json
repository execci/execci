{
  "name": "react-native-node-mongo-typescript-template",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "dev:server": "./scripts/dev-server.sh",
    "dev:client": "yarn install && npm run web",
    "build:graphql": "apollo client:codegen --endpoint=http://localhost:3333/graphql --target=typescript",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest"
  },
  "dependencies": {
    "@apollo/client": "^3.6.9",
    "@react-navigation/bottom-tabs": "^6.3.2",
    "@react-navigation/material-bottom-tabs": "^6.2.2",
    "@react-navigation/native": "^6.0.11",
    "@react-navigation/native-stack": "^6.7.0",
    "@sendgrid/mail": "^7.7.0",
    "babel-plugin-inline-dotenv": "^1.7.0",
    "connect-mongo": "^4.6.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "expo": "~45.0.0",
    "expo-linking": "^3.1.0",
    "expo-splash-screen": "^0.15.1",
    "expo-status-bar": "~1.3.0",
    "expo-web-browser": "^10.2.1",
    "express": "^4.18.1",
    "express-graphql": "^0.12.0",
    "express-http-proxy": "^1.6.3",
    "express-session": "^1.17.3",
    "graphql-compose": "^9.0.8",
    "http-proxy": "^1.18.1",
    "mongodb": "^4.8.1",
    "mongoose": "^6.5.0",
    "morgan": "^1.10.0",
    "passport": "^0.6.0",
    "passport-local-mongoose": "^7.1.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-native": "0.68.2",
    "react-native-paper": "^4.12.4",
    "react-native-safe-area-context": "^4.3.1",
    "react-native-web": "0.17.7"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@types/express-session": "^1.17.5",
    "@types/http-proxy": "^1.17.9",
    "@types/jest": "^28.1.6",
    "@types/mongoose": "^5.11.97",
    "@types/passport-local-mongoose": "^6.1.1",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.66.13",
    "jest": "^28.1.3",
    "jest-expo": "^45.0.1",
    "nodemon": "^2.0.19",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.0.0",
    "typescript": "~4.3.5"
  },
  "private": true,
  "prettier": {
    "proseWrap": "always",
    "singleQuote": true,
    "trailingComma": "all"
  },
  "jest": {
    "preset": "jest-expo",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "verbose": true,
    "rootDir": "",
    "modulePaths": [
      "<rootDir>",
      "<rootDir>/node_modules"
    ]
  }
}
